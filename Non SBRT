                                   "Hipsanic or Latino" = "Hispanic or Latino",
                                   "Not Hispanic or Latino" = "Not Hispanic or Latino",
                                   "Declined" = "Unknown/Declined",
                                   "Unknown" = "Unknown/Declined",
