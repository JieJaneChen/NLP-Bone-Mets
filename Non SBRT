  tidy(exponentiate = TRUE, conf.int = TRUE) %>%        
  mutate(across(where(is.numeric), round, digits = 4))
Pain_lang

dxDemo_pain <- filter(dxDemo_pain, !Race_grouped %in% c("Unknown"))
